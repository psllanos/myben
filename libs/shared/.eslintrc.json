{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {}
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {}
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {}
        },
        {
            "files": ["*.ts"],
            "rules": {
                "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
            }
        }
    ]
}
